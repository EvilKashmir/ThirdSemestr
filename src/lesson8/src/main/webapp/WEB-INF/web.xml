<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
  <servlet>
    <servlet-name>registrationServlet</servlet-name>
    <servlet-class>ru.itis.servlets.RegistrationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>registrationServlet</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>mainServlet</servlet-name>
    <servlet-class>ru.itis.servlets.MainServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>mainServlet</servlet-name>
    <url-pattern>/main</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>registrationFilter</filter-name>
    <filter-class>ru.itis.filters.RegistrationFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>registrationFilter</filter-name>
    <servlet-name>registrationServlet</servlet-name>
  </filter-mapping>
</web-app>